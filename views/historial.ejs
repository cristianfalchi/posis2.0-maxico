<%- include('partials/head', {title: "Maxico Srl - Posis 2.0"}) %>

<body>
    <div class="container">
        <!-- menu de navegacion -->
        <%- include('partials/navbar') %>

        <!-- animacion de uploading -->
        <%- include('partials/uploading', {imgUploading: 'uploading.gif'}) %>
        
        <div class="container__info stock animate__animated animate__fadeIn animate__faster">
            <div class = "title">
                <h2>>Historial</h2>
            <hr/>
            </div>
            <div class="div-table">
                <table >
                    <thead>
                        <tr >
                          <th>sequenceNumber</th>
                          <th>date</th>
                          <th>customers</th>
                          <th>informed</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for( let i = 0; i < sequences.length; i++ ) { %>
                        <tr>
                            <td><%= sequences[i].sequenceNumber %> </td>
                            <td><%= sequences[i].date %></td>
                            <td><%= sequences[i].customers %></td>
                            <td><%= sequences[i].informed %></td>
                        </tr>
                        <% } %>
                    </tbody>
                  </table>
            </div>
        </div>
    </div>

    <script type="text/javascript" >

        function handleSendData() {
            document.querySelector('.uploading').style.display = "block";
        }
    
        </script>

</body>
</html>